{
  "expo": {
    "name": "Key Conservation",
    "slug": "key-conservation",
    "privacy": "unlisted",
    "sdkVersion": "36.0.0",
    "scheme": "keyconservation",
    "platforms": ["ios", "android"],
    "version": "4.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/keyapplogo.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "com.kc.keyconservation",
      "versionCode": 7
    },
    "ios": {
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleTypeRole": "None",
            "CFBundleURLName": "auth0",
            "CFBundleURLSchemes": ["com.kc.keyconservation"]
          }
        ]
      },
      "buildNumber": "9",
      "supportsTablet": true,
      "bundleIdentifier": "com.kc.keyconservation"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "ts", "tsx", "svg"],
      "transformer": "node_modules/react-native-svg-transformer/index.js"
    },
    "description": "The Key Conservation App\n\nVersion 3.2.1",
    "githubUrl": "https://github.com/labs14-key-conservation/Frontend-Mobile"
  }
}
